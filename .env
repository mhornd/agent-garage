# ================================
# Ollama Model Configuration
# ================================

# Primary LLM model to pull and use
# Default: llama3.2
# Examples: llama3.2, llama3.1, mixtral, phi3, codellama, qwen
# See https://ollama.com/library for available models
OLLAMA_MODEL=llama3.2

# Optional: Additional models to pull on startup
# Use COMMA (,) as delimiter for multiple models
# Leave empty if you only need the primary model
# Examples: mixtral,codellama,phi3
OLLAMA_ADDITIONAL_MODELS=qwen3-vl:8b

# ================================
# Database Configuration
# ================================
POSTGRES_USER=root
POSTGRES_PASSWORD=password
POSTGRES_DB=n8n

# ================================
# n8n Configuration
# ================================
N8N_ENCRYPTION_KEY=super-secret-key
N8N_USER_MANAGEMENT_JWT_SECRET=even-more-secret
N8N_DEFAULT_BINARY_DATA_MODE=filesystem

# ================================
# Jira Configuration
# ================================
JIRA_PERSONAL_TOKEN=your_personal_access_token
JIRA_PROJECT=project_key
JIRA_USERNAME=your_jira_username

JIRA_POSTGRES_USER=jira
JIRA_POSTGRES_PASSWORD=jirapassword
JIRA_POSTGRES_DB=jiradb
